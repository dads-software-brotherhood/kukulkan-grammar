entity Usuario (usuarios){
    nombre String required,
    edad Integer,
    numeroCredencial Long,
    sueldo BigDecimal,
    impuesto Float,
    impuestoDetalle Double,
    ativo Boolean,
    fechaLocalDate LocalDate,
    fechaZoneDateTime ZonedDateTime,
    imagen Blob,
    imagenAnyBlob AnyBlob,
    imagenBlob ImageBlob,
    desc TextBlob,
    instante Instant
}

entity Persona {
    nombre String,
    edad Integer,
    numeroCredencial Long,
    sueldo BigDecimal,
    impuesto Float,
    impuestoDetalle Double,
    ativo Boolean,
    fechaLocalDate LocalDate,
    fechaZoneDateTime ZonedDateTime,
    imagen Blob,
    imagenAnyBlob AnyBlob,
    imagenBlob ImageBlob,
    desc TextBlob,
    instante Instant
}